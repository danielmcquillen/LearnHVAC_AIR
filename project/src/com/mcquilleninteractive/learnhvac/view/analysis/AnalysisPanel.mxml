<?xml version="1.0" encoding="utf-8"?>
<mx:VBox xmlns:mx="http://www.adobe.com/2006/mxml" 
	xmlns:view="com.mcquilleninteractive.learnhvac.view.*" 
	xmlns:analysis="com.mcquilleninteractive.learnhvac.view.analysis.*" 
	xmlns:graphing="com.mcquilleninteractive.learnhvac.view.graphing.*"
	width="100%" height="100%">
	
	<mx:Script>
		<![CDATA[
				import com.mcquilleninteractive.learnhvac.model.ScenarioModel;
				import com.mcquilleninteractive.learnhvac.model.LHModelLocator;
				
	        	
			
		]]>
	</mx:Script>
	
	<analysis:AnalysisControlBar width="100%"/>

	<mx:ViewStack id="viewStack"
		width="100%" height="100%"
		selectedIndex="{LHModelLocator.getInstance().scenarioModel.analysisView}"
	    paddingLeft="5"
	    paddingRight="5"
		creationPolicy="all"
		>
		
		<analysis:AllData id="allData" width="100%" height="100%"/>
		<analysis:EnergyOverview id="energyOverview" width="100%" height="100%"/>
		
	</mx:ViewStack>
	
	
	
</mx:VBox>
